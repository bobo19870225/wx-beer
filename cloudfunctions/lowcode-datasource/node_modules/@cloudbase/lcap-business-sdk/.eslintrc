{
  "env": {
    "node": true,
    "es6": true
  },
  // "parser": "@typescript-eslint/parser",
  "extends": [
    "@tencent/eslint-config-tencent",
    "@tencent/eslint-config-tencent/ts"
  ],
  "rules": {
    "complexity": [
      "off"
    ],
    "no-throw-literal": [
      "off"
    ],
    "no-nested-ternary": "off",
    "no-param-reassign": "off",
    "linebreak-style": [
      0,
      "error",
      "windows"
    ]
  }
}