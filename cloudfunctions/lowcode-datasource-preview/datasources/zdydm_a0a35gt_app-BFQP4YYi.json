			"calleeBody":{
				"callee":"const cloudbase = require(\"@cloudbase/node-sdk\");\n\nconst collectionName = \"cloudbase-sample-orders\";\n\nmodule.exports = async function (params, context) {\n  const envId = context.vars.TARGET_ENV || context.env.envId;\n  const app = cloudbase.init({\n    env: envId\n  })\n  const { pageNo = 1, pageSize = 10, orderStatus, name, addressee, phone, orderTime, orderId } = params;\n  const db = app.database();\n\n  const _ = db.command;\n\n  let ninetyDaysAgo = new Date();\n  ninetyDaysAgo.setDate(ninetyDaysAgo.getDate() - 90);\n  ninetyDaysAgo = ninetyDaysAgo.getTime();\n  let thirtyDaysAgo = new Date();\n  thirtyDaysAgo.setDate(thirtyDaysAgo.getDate() - 30);\n  thirtyDaysAgo = thirtyDaysAgo.getTime();\n  let sevenDaysAgo = new Date();\n  sevenDaysAgo.setDate(sevenDaysAgo.getDate() - 7);\n  sevenDaysAgo = sevenDaysAgo.getTime();\n  let today = new Date();\n  today.setDate(today.getDate() + 1);\n  today.setHours(0, 0, 0, 0);\n  today = today.getTime();\n\n  const orderTimeRange = {\n    '1': today,\n    '2': sevenDaysAgo,\n    '3': thirtyDaysAgo,\n    '4': ninetyDaysAgo\n  }\n\n  let qs = Object.assign(\n    {},\n    name && {\n      name: new db.RegExp({regexp: name})\n    },\n    phone && {\n      receivePhone: new db.RegExp({regexp: phone})\n    },\n    orderTime && {\n      orderTime: _.gte(orderTimeRange[orderTime])\n    },\n    orderId && {\n      orderId: new db.RegExp({regexp: orderId})\n    },\n    orderStatus && {\n      orderStatus\n    },\n    addressee && {\n      receiver: addressee\n    } \n  )\n\n  const query = db.collection(collectionName).where(qs);\n\n  const recordsRes = await query.skip((pageNo - 1) * pageSize)\n  .limit(pageSize).get();\n  const totalRes = await query.count();\n  console.log(recordsRes.data)\n\n  return {\n    records: recordsRes.data || [],\n    total: totalRes.total,\n    pageNo,\n    pageSize\n  }\n};",
